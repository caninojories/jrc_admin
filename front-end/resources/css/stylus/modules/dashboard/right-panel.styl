.rightPanel {
  position                : fixed;
  top                     : 50px;
  right                   : 0;
  width                   : 400px;
  height                  : 100%;
  padding-left            : 5px;
  background              :#2a3542;
  border-top-left-radius  : 10px;
  overflow-y              : auto;
  .information {
    > h5 {
      padding-left        : 10px;
    }
    > p{
      padding-left        : 20px;
    }
  }
}